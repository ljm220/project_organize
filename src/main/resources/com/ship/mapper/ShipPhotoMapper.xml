<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
   PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
 <mapper namespace="com.ship.mapper.ShipPhotoMapper">
  
  	<insert id="insertShipPhoto" parameterType="ShipPhoto">
  		insert into SHIP_PHOTO values (SHIP_PHOTO_SEQ.nextval,#{shipPhotoName},#{shipOriginPhotoName},1,SYSDATE,#{shipNum})
	</insert>
	
 </mapper> 